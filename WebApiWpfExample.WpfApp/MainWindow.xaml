<Window x:Class="WebApiWpfExample.WpfApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WebApiWpfExample.WpfApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Padding="10">
    <Grid>
        <DataGrid AutoGenerateColumns="True" Name="dgWeatherData" Margin="0,35,0,0" ItemsSource="{Binding WeatherData}">
        </DataGrid>
        <Button Content="Load Data" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="80" Command="{Binding LoadData}"/>
    </Grid>
</Window>
